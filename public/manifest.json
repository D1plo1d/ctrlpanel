{
  "name": "Ctrl Panel",
  "description": "Your 3D printer, now with the power of the web.",
  "version": "0.0.0.0",
  "manifest_version": 2,
  "app": {
    "background": {
        "scripts": ["js/background.js"]
      }
  },
  "requirements": {
    "3D": {
      "features": ["webgl"]
    }
  },
  "icons": {
    "128": "icon_128.png"
  },
  "permissions": [{
    "socket": [
      "udp-send-to::99"
    ]
  }]
}